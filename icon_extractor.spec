# PyInstaller spec file for EU Icon Extractor
# Usage: pyinstaller icon_extractor.spec

block_cipher = None

a = Analysis(
    ['icon_extractor.py'],
    pathex=[],
    binaries=[],
    datas=[('icon.ico', '.'), ('assets/icon.ico', 'assets')],
    hiddenimports=[],
    hookspath=[],
    hooksconfig={},
    runtime_hooks=[],
    excludes=[],
    win_no_prefer_redirects=False,
    win_private_assemblies=False,
    cipher=block_cipher,
    noarchive=False,
